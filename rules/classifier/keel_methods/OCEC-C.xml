<algorithm_specification>
  <name>Organizational Coevolutionary Algorithm (OCEC)</name>
  <nParameters>2</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Number of Total Generations</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>500</default>
  </parameter>
  <parameter>
    <name>Number of migrating/exchanging members</name>
    <type>integer</type>
      <lowerB>1</lowerB>
      <upperB>10</upperB>    
    <default>1</default>
  </parameter>
</algorithm_specification>
