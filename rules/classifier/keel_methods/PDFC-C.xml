<algorithm_specification>
  <name>Positive Definite Fuzzy Classifier</name>
  <nParameters>7</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>C</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>100.0</default>
  </parameter>
  <parameter>
    <name>d</name>
    <type>real</type>
    <domain>
      <lowerB>1.0E-10</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.25</default>
  </parameter>
  <parameter>
    <name>toleranceParameter</name>
    <type>real</type>
    <domain>
      <lowerB>1.0E-11</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>0.001</default>
  </parameter>
   <parameter>
    <name>epsilon</name>
    <type>real</type>
    <domain>
      <lowerB>1.0E-12</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>1.0E-12</default>
  </parameter>
  <parameter>
    <name>PDRFtype</name>
    <type>list</type>
    <domain>
      <item>SymmetricTriangle</item>
      <item>Gaussian</item>
      <item>Cauchy</item>
      <item>Laplace</item>
      <item>HyperbolicSecant</item>
      <item>SquaredSinc</item>
    </domain>
    <default>Gaussian</default>
  </parameter>
  <parameter>
    <name>ConvertNominalAttributesToBinary</name>
    <type>list</type>
    <domain>
      <item>True</item>
      <item>False</item>
    </domain>
    <default>True</default>
  </parameter>
  <parameter>
    <name>PreprocessType</name>
    <type>list</type>
    <domain>
      <item>Normalize</item>
      <item>Standardize</item>
      <item>None</item>
    </domain>
    <default>Normalize</default>
  </parameter>
</algorithm_specification>

