<algorithm_specification>
  <name>Fuzzy Rule Learning, SLAVEv0 Algorithm</name>
  <nParameters>5</nParameters>
  <seed>1</seed>
  <nOutput>2</nOutput>
  <parameter>  
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>20</default>
  </parameter>
  <parameter>
    <name>Number of Iterations Allowed without Change</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>500</default>
  </parameter>
    <parameter>
    <name>Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
      <parameter>
    <name>Crossover Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Lambda</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.8</default>
  </parameter>
</algorithm_specification>
