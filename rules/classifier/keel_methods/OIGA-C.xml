<algorithm_specification>
  <name>OIGA</name>
  <nParameters>8</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>ProbMutation</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
   <parameter>
    <name>CrossoverRate</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>1.0</default>
  </parameter>
  <parameter>
    <name>popSize</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>200</default>
  </parameter>
  <parameter>
    <name>ruleNumber</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>30</default>
  </parameter>
  <parameter>
    <name>stagnationLimit</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>30</default>
  </parameter>
  <parameter>
    <name>generationLimit</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>500</upperB>
    </domain>
    <default>200</default>
  </parameter>
  <parameter>
    <name>SurvivorsPercent</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>AttributeOrder</name>
    <type>list</type>
    <domain>
      <item>descendent</item>
      <item>ascendent</item>
      <item>original</item>
      <item>random</item>
    </domain>
    <default>descendent</default>
  </parameter>
</algorithm_specification>
